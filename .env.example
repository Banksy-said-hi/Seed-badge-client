# Database Configuration
DATABASE_URL="postgres://username:password@host:port/database?sslmode=require"

# Infura API Key (get from https://infura.io)
NEXT_PUBLIC_INFURA_API_KEY="your_infura_api_key"

# Pimlico API Key (get from https://dashboard.pimlico.io)
NEXT_PUBLIC_PIMLICO_API_KEY="your_pimlico_api_key"

# Web3Auth Client ID (get from https://dashboard.web3auth.io)
NEXT_PUBLIC_WEB3AUTH_CLIENT_ID="your_web3auth_client_id"

# OAuth Client ID for Klang/FusionAuth
NEXT_PUBLIC_OAUTH_CLIENT_ID="your_oauth_client_id"